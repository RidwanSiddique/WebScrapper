{
  "dependencies": {
    "puppeteer": "^24.26.0"
  },
  "name": "puppeteer",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "tsx index.ts",
    "start:visual": "APP_VISUAL=1 tsx index.ts",
    "start:single": "MAX_PAGES=1 tsx index.ts",
    "start:3pages": "MAX_PAGES=3 tsx index.ts",
    "start:5pages": "MAX_PAGES=5 tsx index.ts",
    "start:all": "MAX_PAGES=10 tsx index.ts",
    "start:single:visual": "MAX_PAGES=1 APP_VISUAL=1 tsx index.ts",
    "start:3pages:visual": "MAX_PAGES=3 APP_VISUAL=1 tsx index.ts",
    "start:5pages:visual": "MAX_PAGES=5 APP_VISUAL=1 tsx index.ts",
    "start:all:visual": "MAX_PAGES=10 APP_VISUAL=1 tsx index.ts",
    "dev": "APP_DEBUG=1 APP_VISUAL=1 tsx index.ts",
    "dev:debug": "APP_DEBUG=1 node --inspect-brk ./node_modules/.bin/tsx index.ts",
    "dev:win": "set APP_DEBUG=1&& tsx index.ts",
    "debug:proto": "APP_DEBUG=1 DEBUG=puppeteer:* node --inspect-brk ./node_modules/.bin/tsx index.ts"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
